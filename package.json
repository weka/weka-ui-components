{
  "name": "@weka/weka-ui-components",
  "repository": {
    "type": "git",
    "url": "https://github.com/weka/weka-ui-components.git"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "type": "module",
  "version": "4.0.0-beta.2",
  "module": "lib/main.ts",
  "types": "lib/main.ts",
  "exports": {
    ".": {
      "import": "./lib/main.ts",
      "types": "./lib/main.ts"
    },
    "./v2": {
      "types": "./dist/v2/index.d.ts",
      "development": "./lib/v2/index.ts",
      "default": "./dist/v2/index.js"
    }
  },
  "sideEffects": [
    "*.css",
    "*.scss"
  ],
  "scripts": {
    "build": "vite build",
    "test": "vitest",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build -c .storybook",
    "predeploy-storybook": "yarn run build-storybook",
    "deploy-storybook": "gh-pages -d storybook-static",
    "lint": "eslint ./lib",
    "format": "prettier --write .",
    "release:patch": "npm version patch && git push -u origin HEAD && git push --tags",
    "release:minor": "npm version minor && git push -u origin HEAD && git push --tags",
    "release:major": "npm version major && git push -u origin HEAD && git push --tags",
    "release:beta": "npm version prerelease --preid beta && git push -u origin HEAD && git push --tags"
  },
  "files": [
    "lib",
    "dist"
  ],
  "dependencies": {
    "@tanstack/react-table": "^8.14.0",
    "@tanstack/react-virtual": "^3.11.2",
    "ace-builds": "^1.36.2",
    "check-password-strength": "^2.0.7",
    "clsx": "^2.0.0",
    "copy-to-clipboard": "^3.3.3",
    "luxon": "^3.4.4",
    "react-ace": "^12.0.0",
    "react-hook-form": "^7.53.2",
    "react-markdown": "^10.0.0",
    "react-router-dom": "^6.30.3",
    "react-select": "^5.8.0",
    "react-toastify": "^9.1.3",
    "react-use": "^17.5.1",
    "simplebar-react": "^3.3.0"
  },
  "peerDependencies": {
    "@emotion/react": "^11.10.5",
    "@emotion/styled": "^11.10.5",
    "@mui/material": "^5.11.1",
    "luxon": "^3.4.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.6.2"
  },
  "devDependencies": {
    "@babel/core": "^7.16.0",
    "@babel/preset-env": "^7.16.4",
    "@babel/preset-react": "^7.16.0",
    "@babel/preset-typescript": "^7.16.0",
    "@chromatic-com/storybook": "^2.0.2",
    "@emotion/react": "^11.10.5",
    "@emotion/styled": "^11.10.5",
    "@mui/material": "^5.11.1",
    "@storybook/addon-actions": "^8.6.14",
    "@storybook/addon-essentials": "^8.6.14",
    "@storybook/addon-links": "^8.6.14",
    "@storybook/addon-webpack5-compiler-babel": "^3.0.3",
    "@storybook/preset-scss": "^1.0.3",
    "@storybook/react": "^8.6.14",
    "@storybook/react-webpack5": "^8.6.14",
    "@storybook/test": "^8.6.14",
    "@svgr/webpack": "^8.1.0",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.2",
    "@types/jsdom": "^27",
    "@types/luxon": "^3.2.0",
    "@types/react": "^18.0.15",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^4.3.1",
    "@weka/eslint-config": "https://github.com/weka/eslint-config#1.1.0",
    "babel-loader": "^9.1.3",
    "eslint": "^8.34.0",
    "gh-pages": "^5.0.0",
    "happy-dom": "^20.3.9",
    "jsdom": "^27.4.0",
    "prettier": "^2.8.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
