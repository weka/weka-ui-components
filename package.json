{
  "name": "@weka/weka-ui-components",
  "repository": {
    "type": "git",
    "url": "https://github.com/weka/weka-ui-components.git"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "type": "module",
  "version": "4.0.0-beta.1",
  "module": "lib/main.ts",
  "types": "lib/main.ts",
  "exports": {
    ".": {
      "import": "./lib/main.ts",
      "types": "./lib/main.ts"
    },
    "./v2": {
      "types": "./dist/v2/index.d.ts",
      "development": "./lib/v2/index.ts",
      "default": "./dist/v2/index.js"
    },
    "./v2/styles.css": "./dist/assets/Button.module.css"
  },
  "sideEffects": [
    "*.css",
    "*.scss"
  ],
  "scripts": {
    "build": "vite build",
    "test": "vitest",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build -c .storybook",
    "predeploy-storybook": "yarn run build-storybook",
    "deploy-storybook": "gh-pages -d storybook-static",
    "typecheck": "tsc -p tsconfig.v2.json",
    "lintcheck": "eslint lib/v2",
    "lint": "eslint lib/v2 --fix",
    "lint:diff": "ESLINT_PLUGIN_DIFF_COMMIT=origin/main eslint lib/v2 --config eslint.diff.config.mjs",
    "lint:diff:fix": "ESLINT_PLUGIN_DIFF_COMMIT=origin/main eslint lib/v2 --fix --config eslint.diff.config.mjs",
    "formatcheck": "prettier --check 'lib/v2/**/*'",
    "format": "prettier --write 'lib/v2/**/*'",
    "stylecheck": "stylelint 'lib/v2/**/*.scss'",
    "stylefix": "stylelint 'lib/v2/**/*.scss' --fix",
    "checkall": "yarn typecheck && yarn lintcheck && yarn stylecheck && yarn formatcheck && yarn test:run",
    "check-ci": "./scripts/local-ci-check.sh",
    "release:patch": "yarn version patch && git push && git push --tags",
    "release:minor": "yarn version minor && git push && git push --tags",
    "release:major": "yarn version major && git push && git push --tags"
  },
  "files": [
    "lib",
    "dist"
  ],
  "dependencies": {
    "@tanstack/react-table": "^8.14.0",
    "@tanstack/react-virtual": "^3.11.2",
    "ace-builds": "^1.36.2",
    "check-password-strength": "^2.0.7",
    "clsx": "^2.0.0",
    "copy-to-clipboard": "^3.3.3",
    "luxon": "^3.4.4",
    "react-ace": "^12.0.0",
    "react-hook-form": "^7.53.2",
    "react-markdown": "^10.0.0",
    "react-router-dom": "^6.30.3",
    "react-select": "^5.8.0",
    "react-toastify": "^9.1.3",
    "react-use": "^17.5.1",
    "simplebar-react": "^3.3.0"
  },
  "peerDependencies": {
    "@emotion/react": "^11.10.5",
    "@emotion/styled": "^11.10.5",
    "@mui/material": "^5.11.1",
    "luxon": "^3.4.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.6.2"
  },
  "devDependencies": {
    "@babel/core": "^7.16.0",
    "@babel/preset-env": "^7.16.4",
    "@babel/preset-react": "^7.16.0",
    "@babel/preset-typescript": "^7.16.0",
    "@chromatic-com/storybook": "^2.0.2",
    "@emotion/react": "^11.10.5",
    "@emotion/styled": "^11.10.5",
    "@eslint/compat": "^2.0.1",
    "@eslint/js": "^9.39.2",
    "@mui/material": "^5.11.1",
    "@storybook/addon-actions": "^8.6.14",
    "@storybook/addon-essentials": "^8.6.14",
    "@storybook/addon-links": "^8.6.14",
    "@storybook/addon-webpack5-compiler-babel": "^3.0.3",
    "@storybook/preset-scss": "^1.0.3",
    "@storybook/react": "^8.6.14",
    "@storybook/react-webpack5": "^8.6.14",
    "@storybook/test": "^8.6.14",
    "@svgr/webpack": "^8.1.0",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.2",
    "@types/eslint-plugin-jsx-a11y": "^6",
    "@types/jsdom": "^27",
    "@types/luxon": "^3.2.0",
    "@types/react": "^18.0.15",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^4.3.1",
    "@vitest/coverage-v8": "^4.0.18",
    "@weka/eslint-plugin-weka": "https://github.com/weka/eslint-plugin-weka.git",
    "babel-loader": "^9.1.3",
    "eslint": "^9.0.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-diff": "^2.0.3",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-promise": "^7.2.1",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.26",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-sonarjs": "^3.0.6",
    "eslint-plugin-unused-imports": "^4.3.0",
    "gh-pages": "^5.0.0",
    "globals": "^17.1.0",
    "happy-dom": "^20.3.9",
    "jsdom": "^27.4.0",
    "postcss": "^8.5.6",
    "prettier": "^2.8.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "sass": "^1.43.5",
    "sass-loader": "^14.2.1",
    "storybook": "^8.6.15",
    "storybook-solidjs": "^1.0.0-beta.2",
    "stylelint": "^17.0.0",
    "stylelint-config-standard-scss": "^17.0.0",
    "tsconfig-paths-webpack-plugin": "^4.1.0",
    "typescript": "5.1.6",
    "typescript-eslint": "^8.54.0",
    "url-loader": "^4.1.1",
    "vite": "^6.4.1",
    "vite-plugin-dts": "^4.5.4",
    "vite-plugin-lib-inject-css": "^2.1.1",
    "vite-plugin-svgr": "^4.2.0",
    "vite-tsconfig-paths": "^5.1.2",
    "vitest": "^4.0.18"
  },
  "resolutions": {
    "brace-expansion": "2.0.2",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "packageManager": "yarn@4.6.0"
}
