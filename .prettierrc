{
  "semi": false,
  "jsxSingleQuote": true,
  "singleQuote": true,
  "bracketSpacing": true,
  "trailingComma": "none",
  "bracketSameLine": false,
  "printWidth": 100,
  "singleAttributePerLine": true,
  "overrides": [
    {
      "files": ["*.js", "*.jsx", "*.ts", "*.tsx", "*.json", "**/*.test.*"],
      "options": {
        "printWidth": 80
      }
    }
  ]
}
